{
  "datasaur": {
    "host": "https://app.datasaur.ai",
    "clientId": "<DATASAUR_CLIENT_ID>",
    "clientSecret": "<DATASAUR_CLIENT_SECRET>"
  },
  "projectState": {
    "source": "local",
    "path": "quickstart/state/projectState.json"
  },
  "export": {
    "source": "local",
    "prefix": "quickstart/row-based/export",
    "teamId": "<TEAM_ID>",
    "statusFilter": [],
    "format": "CSV",
    "customScriptId": null
  },
  "create": {
    "teamId": "<TEAM_ID>",
    "files": {
      "source": "local",
      "path": "quickstart/row-based/document"
    },
    "assignment": {
      "source": "local",
      "path": "quickstart/row-based/config/assignment.json"
    },
    "projectSettings": {
      "consensus": 1,
      "enableEditLabelSet": false,
      "enableEditSentence": false,
      "hideLabelerNamesDuringReview": false,
      "hideRejectedLabelsDuringReview": true,
      "hideLabelsFromInactiveLabelSetDuringReview": false
    },
    "documentSettings": {
      "kind": "ROW_BASED",
      "customScriptId": null,
      "displayedRows": 10,
      "mediaDisplayStrategy": "THUMBNAIL",
      "firstRowAsHeader": true,
      "viewer": "TABULAR",
      "viewerConfig": {},
      "enableTabularMarkdownParsing": false
    },
    "questionSetFile": "quickstart/row-based/config/question-set.json",
    "docFileOptions": {
      "firstRowAsHeader": true,
      "customHeaderColumns": [
        {
          "name": "Shown to all",
          "displayed": true,
          "labelerRestricted": false
        },
        {
          "name": "Hidden from Labeler",
          "displayed": true,
          "labelerRestricted": true
        },
        {
          "name": "Hidden from all",
          "displayed": false,
          "labelerRestricted": true
        },
        {
          "name": "Hidden from all",
          "displayed": false,
          "labelerRestricted": false
        },
        {
          "name": "Year",
          "displayed": true,
          "labelerRestricted": false
        },
        {
          "name": "City",
          "displayed": true,
          "labelerRestricted": false
        },
        {
          "name": "Code",
          "displayed": false,
          "labelerRestricted": false
        },
        {
          "name": "Location",
          "displayed": false,
          "labelerRestricted": false
        },
        {
          "name": "State",
          "displayed": false,
          "labelerRestricted": false
        },
        {
          "name": "Avg Temp",
          "displayed": true,
          "labelerRestricted": false
        },
        {
          "name": "Max Temp",
          "displayed": false,
          "labelerRestricted": false
        },
        {
          "name": "Min Temp",
          "displayed": false,
          "labelerRestricted": false
        },
        {
          "name": "Wind.Direction",
          "displayed": true,
          "labelerRestricted": false
        },
        {
          "name": "Wind.Speed",
          "displayed": true,
          "labelerRestricted": false
        }
      ]
    }
  }
}
