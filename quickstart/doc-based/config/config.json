{
  "datasaur": {
    "host": "https://app.datasaur.ai",
    "clientId": "<DATASAUR_CLIENT_ID>",
    "clientSecret": "<DATASAUR_CLIENT_SECRET>"
  },
  "projectState": {
    "source": "local",
    "path": "quickstart/state/doc-project-state.json"
  },
  "export": {
    "source": "local",
    "prefix": "quickstart/doc-based/export",
    "teamId": "<TEAM_ID>",
    "statusFilter": [],
    "format": "CSV",
    "customScriptId": null
  },
  "create": {
    "teamId": "<TEAM_ID>",
    "files": {
      "source": "local",
      "path": "quickstart/doc-based/document"
    },
    "assignment": {
      "source": "local",
      "path": "quickstart/doc-based/config/assignment.json"
    },
    "projectSettings": {
      "consensus": 1,
      "enableEditLabelSet": false,
      "enableEditSentence": false,
      "hideLabelerNamesDuringReview": false,
      "hideRejectedLabelsDuringReview": true,
      "hideLabelsFromInactiveLabelSetDuringReview": false
    },
    "documentSettings": {
      "kind": "DOCUMENT_BASED",
      "customScriptId": null,
      "displayedRows": -1,
      "mediaDisplayStrategy": "THUMBNAIL",
      "firstRowAsHeader": false,
      "viewer": "URL",
      "viewerConfig": {
        "urlColumnNames": []
      },
      "enableTabularMarkdownParsing": false
    },
    "questionSetFile": "quickstart/doc-based/config/question-set.json",
    "docFileOptions": {
      "firstRowAsHeader": false,
      "customHeaderColumns": [
        {
          "displayed": true,
          "labelerRestricted": false,
          "name": "Column 1"
        }
      ]
    }
  }
}
