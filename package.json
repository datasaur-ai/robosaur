{
  "name": "robosaur",
  "version": "1.0.0",
  "description": "Datasaur template for creating and exporting projects.",
  "main": "src/index.ts",
  "scripts": {
    "test:ci": "jest --runInBand --ci",
    "test": "jest",
    "start": "ts-node src/index.ts",
    "codegen": "graphql-codegen --config codegen.yml",
    "build": "rimraf dist/ && tsc --project tsconfig.json",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@azure/storage-blob": "~12.9.0",
    "@google-cloud/storage": "5.18.0",
    "adm-zip": "0.5.9",
    "ajv": "8.9.0",
    "axios": "0.21.4",
    "commander": "7.2.0",
    "dotenv": "10.0.0",
    "graphql": "15.8.0",
    "graphql-request": "3.7.0",
    "graphql-upload": "12.0.0",
    "lodash": "4.17.21",
    "minio": "7.0.26",
    "papaparse": "5.3.1",
    "qs": "6.10.3",
    "winston": "3.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.7",
    "@types/adm-zip": "0.4.34",
    "@types/graphql": "^14.5.0",
    "@types/jest": "27.4.0",
    "@types/minio": "7.0.11",
    "@types/node": "16.11.21",
    "@types/papaparse": "5.3.1",
    "eslint": "8.7.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "26.6.3",
    "jest-config": "26.6.3",
    "nodemon": "2.0.15",
    "onchange": "7.1.0",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "26.5.6",
    "ts-loader": "5.4.5",
    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  },
  "author": "Datasaur.ai (https://github.com/datasaur-ai)",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": [
      ".spec.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest",
      "^.+\\.(ts|js)x?$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
